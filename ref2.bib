shorttitle = {High-frequency percussive ventilation and low tida},
title = {{High-frequency percussive ventilation and low tidal volume ventilation in burns: A randomized controlled trial}},
url = {http://content.wkhealth.com/linkback/openurl?sid=WKPTLP:landingpage{\&}an=00003246-201010000-00008 http://ovidsp.tx.ovid.com/ovftpdfs/FPDDNCIBHFCJOM00/fs046/ovft/live/gv023/00003246/00003246-201010000-00008.pdf},
